{
  "name": "recaptcha-promise",
  "version": "2.0.0",
  "description": "Node module for promise-based ReCAPTCHA verification",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.test.*\"",
    "lint": "eslint --ignore-path .gitignore . && tsc --noEmit",
    "lint-fix": "eslint --fix --ignore-path .gitignore . && tsc --noEmit",
    "coverage": "nyc --reporter=lcov npm test",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/recaptcha-promise.git"
  },
  "keywords": [
    "recaptcha",
    "captcha"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/recaptcha-promise/issues"
  },
  "homepage": "https://github.com/meyfa/recaptcha-promise",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@types/mocha": "9.1.0",
    "@types/node": "16.11.21",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "axios-mock-adapter": "1.20.0",
    "eslint": "7.32.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "mocha": "9.2.0",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  }
}
